{
    "name": "hamjoint/bramley",
    "description": "Amateur radio contest management.",
    "keywords": ["amateur", "radio", "contest", "competition"],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Tom Pitcher",
            "email": "tom@hamjoint.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hamjoint/bramley/issues",
        "source": "https://github.com/hamjoint/bramley"
    },
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "^5.0",
        "pda/pheanstalk": "^3.0",
        "erusev/parsedown": "^1.5",
        "tjbp/tablelegs": "@dev",
        "tjbp/countries": "@dev",
        "dansmith/laravel5-foundation": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "psr-4": {
            "Hamjoint\\Bramley\\": "src/lib/",
            "Illuminate\\": "../../php/laravel-framework/src/Illuminate/"
        },
        "files": [
            "src/includes/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "src/tests/TestCase.php"
        ]
    }
}
